{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      // "@/*": ["./src/*"]
      "@/*": ["./src/*"]
      // "@components/*": ["./components/*"]
      // "@/lib/*": ["lib/*"]
    },
    "allowJs": true,
    "esModuleInterop": true,
    "jsx": "react-native",
    "lib": ["es6"],
    "moduleResolution": "node",
    "noEmit": true,
    "strict": true,
    "target": "esnext"
  },
  "exclude": ["node_modules"],
  "extends": "expo/tsconfig.base"
}
